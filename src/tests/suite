            exit 77;;
   0) ;;
   *) echo "basic.at:395: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

mkdir project
$at_traceoff
echo "basic.at:395: cvs -Q add project"
echo basic.at:395 >$at_check_line_file
( $at_traceon; cvs -Q add project ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
