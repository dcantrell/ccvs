" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:573: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# FIXCVS:
# See if CVS can correctly notice when an invalid numeric
# revision is specified.
# AT_CHECK([cvs diff -r 1.5 -r 1.7 sfile2],,,
# [error msg
# ])

# Test that we recurse into the correct directory when checking
# for existing files, even if co -d is in use.
cd ..
touch project/extra
$at_traceoff
echo "basic.at:589: cvs -q co -d project1 project"
echo basic.at:589 >$at_check_line_file
( $at_traceon; cvs -q co -d project1 project ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "U project1/Emptydir/sfile1
U project1/sdir2/sfile2
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:589: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# FIXCVS?  The next test used to check things out like this:
#   U newdir/Emptydir/sfile1
#   U newdir/sdir2/sfile2
# but that's difficult to do.  The whole "shorten" thing
# is pretty bogus, because it will break on things
# like "cvs co foo/bar baz/quux".  Unless there's some
# pretty detailed expansion and analysis of the command-line
# arguments, we shouldn't do "shorten" stuff at all.
#
# DRP: I'm not sure how detailed it would need to be - either all the path
# prefixes are the same, in which case the matching parts are shortened, or
# they are not, in which case they are grouped:
#
#   cvs co -d short project/Emptydir project/sdir2
#   U short/Emptydir/sfile1
#   U short/sdir2
#
#   cvs co -d group foo/bar baz/quux
#   U group/foo/bar/file1
#   U group/baz/quux/file2
#
# How do you group under project if you do want to?  How about:
#
#   cvs co -d group/project project/Emptydir project/sdir2
#   U group/project/Emptydir/sfile1
#   U group/project/sdir2/sfile2
#
$at_traceoff
echo "basic.at:621: cvs -q co -d newdir -r release-1 project/Emptydir project/sdir2"
echo basic.at:621 >$at_check_line_file
( $at_traceon; cvs -q co -d newdir -r release-1 project/Emptydir project/sdir2 ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "U newdir/project/Emptydir/sfile1
U newdir/project/sdir2/sfile2
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:621: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# Verify that the top-level CVS directory is not created by default.
$at_traceoff
echo "basic.at:624: test -d CVS"
echo basic.at:624 >$at_check_line_file
( $at_traceon; test -d CVS ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:624: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


