$SPROG add: use \`$SPROG commit' to add this file permanently
" | $at_diff - $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:496: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


$at_traceoff
echo "basic.at:507: cvs status sfile1"
echo basic.at:507 >$at_check_line_file
( $at_traceon; cvs status sfile1 ) >$at_stdout 2>$at_stder1
