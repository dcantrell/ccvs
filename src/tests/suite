   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:175: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


sed \
    -e "s/\<[0-9][0-9]* [a-zA-Z][a-zA-Z]* [0-9][0-9]* [0-9:][0-9:]* -0000\>/RFCDATE/" \
    -e "s/^P /U /" \
    <stdout >nstdout
rm stdout

$at_traceoff
echo "basic.at:175: (cat nstdout; cat stderr >&2)"
echo basic.at:175 >$at_check_line_file
( $at_traceon; (cat nstdout; cat stderr >&2) ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "Index: sdir/ssdir/ssfile
===================================================================
RCS file: $CVSROOT_DIR/project/sdir/ssdir/ssfile,v
retrieving revision 1.1
diff -c -r1.1 ssfile
*** sdir/ssdir/ssfile	RFCDATE	1.1
--- sdir/ssdir/ssfile	RFCDATE
***************
*** 1 ****
--- 1,2 ----
  ssfile
+ ssfile line 2
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:175: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

rm stderr nstdout

$at_traceoff
echo "basic.at:185: cvs -q diff -c -rBASE -C3isacrowd"
echo basic.at:185 >$at_check_line_file
( $at_traceon; cvs -q diff -c -rBASE -C3isacrowd ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:185: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


