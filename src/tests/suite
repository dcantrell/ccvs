            exit 77;;
   1) ;;
   *) echo "basic.at:465: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


$at_traceoff
echo "basic.at:468: cat second-dir/CVS/Repository"
echo basic.at:468 >$at_check_line_file
( $at_traceon; cat second-dir/CVS/Repository ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "second-dir
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:468: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


      $at_traceoff
      $at_times_p && times >$at_times_file
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat $at_status_file`
    ;;

  42 ) # 42. basic.at:474: Emptydir not special-r-l
    at_setup_line='basic.at:474'
    at_desc='Emptydir not special-r-l'
    $at_quiet $ECHO_N " 42: Emptydir not special-r-l                     $ECHO_C"
    at_xfail=no
    (
      echo "42. basic.at:474: testing ..."
      $at_traceon
