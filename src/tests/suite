at_failed=false
echo >>$at_stderr; echo "$SPROG add: scheduling file \`topfile' for addition
$SPROG add: use \`$SPROG commit' to add this file permanently
" | $at_diff - $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:432: exit code was $at_status, expected 0"
      at_failed=:;;
