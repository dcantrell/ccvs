  exit 1
fi

$at_traceon


# If memory serves, some systems (AIX?) have a different message than
# "No such file or directory".  When rediscovered, they will need to be
# accomodated below.
$at_traceoff
echo "basic.at:272: cvs -q -d \$at_group_dir/nonexist update"
echo basic.at:272 >$at_check_line_file
( $at_traceon; cvs -q -d $at_group_dir/nonexist update ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:272: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


sed \
    -e "s/\<[0-9][0-9]* [a-zA-Z][a-zA-Z]* [0-9][0-9]* [0-9:][0-9:]* -0000\>/RFCDATE/" \
    -e "s/^P /U /" \
    <stdout >nstdout
rm stdout

$at_traceoff
echo "basic.at:272: (cat nstdout; cat stderr >&2)"
echo basic.at:272 >$at_check_line_file
( $at_traceon; (cat nstdout; cat stderr >&2) ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo >>$at_stderr; echo "$CPROG [update aborted]: $at_group_dir/nonexist/CVSROOT: No such file or directory
" | $at_diff - $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:272: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

rm stderr nstdout

# Test resurrecting with strange revision numbers
$at_traceoff
echo "basic.at:278: cvs rm -f ssfile"
echo basic.at:278 >$at_check_line_file
( $at_traceon; cvs rm -f ssfile ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:278: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


sed \
    -e "s/\<[0-9][0-9]* [a-zA-Z][a-zA-Z]* [0-9][0-9]* [0-9:][0-9:]* -0000\>/RFCDATE/" \
    -e "s/^P /U /" \
    <stdout >nstdout
