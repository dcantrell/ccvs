echo >>$at_stdout; echo "Checking in ssfile;
$CVSROOT_DIR/project/sdir/ssdir/ssfile,v  <--  ssfile
new revision: 3.1; previous revision: 2.0
done
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:240: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# Test using -r to create a branch
$at_traceoff
echo "basic.at:249: cvs -q ci -m bogus -r 3.0.0"
echo basic.at:249 >$at_check_line_file
( $at_traceon; cvs -q ci -m bogus -r 3.0.0 ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:249: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


sed \
    -e "s/\<[0-9][0-9]* [a-zA-Z][a-zA-Z]* [0-9][0-9]* [0-9:][0-9:]* -0000\>/RFCDATE/" \
    -e "s/^P /U /" \
    <stdout >nstdout
rm stdout

$at_traceoff
echo "basic.at:249: (cat nstdout; cat stderr >&2)"
echo basic.at:249 >$at_check_line_file
( $at_traceon; (cat nstdout; cat stderr >&2) ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo >>$at_stderr; echo "$SPROG commit: $CVSROOT_DIR/project/sdir/ssdir/ssfile,v: can't find branch point 3.0
$SPROG commit: could not check in ssfile
" | $at_diff - $at_stderr || at_failed=:
echo >>$at_stdout; echo "Checking in ssfile;
$CVSROOT_DIR/project/sdir/ssdir/ssfile,v  <--  ssfile
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:249: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

rm stderr nstdout

$at_traceoff
echo "basic.at:256: cvs -q ci -m valid -r 3.1.2"
