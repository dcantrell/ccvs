deleting revision 1.1
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:711: exit code was $at_status, expected 1"
      at_failed=:;;
