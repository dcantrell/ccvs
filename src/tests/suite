echo "basic.at:191: cvs -Q add project"
echo basic.at:191 >$at_check_line_file
( $at_traceon; cvs -Q add project ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
