            exit 77;;
   0) ;;
   *) echo "basic.at:395: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


#
# Create the default project
#
mkdir top
cd top
$at_traceoff
echo "basic.at:395: cvs -Q co -l ."
echo basic.at:395 >$at_check_line_file
( $at_traceon; cvs -Q co -l . ) >$at_stdout 2>$at_stder1
