   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "init.at:16: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

