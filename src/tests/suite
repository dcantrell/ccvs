    at_setup_line='basic.at:419'
    at_desc='top of repository-l'
    $at_quiet $ECHO_N " 23: top of repository-l                          $ECHO_C"
    at_xfail=no
    (
      echo "23. basic.at:419: testing ..."
      $at_traceon
###
### Begin AT_CVS_SETUP([top of repository])
