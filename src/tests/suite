###


#
# Set CVSROOT_DIR, method, & CVSROOT
#
CVSROOT_DIR=$at_group_dir/cvsroot
$at_traceoff
echo "version.at:16: cvs --version |grep client || exit 77"
echo version.at:16 >$at_check_line_file
