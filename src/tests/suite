( $at_traceon; $as_cvs_have_username || exit 77 ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:386: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# Set $nsrname to $username normalized to account for the fact that most CVS
# output is optimized to print $username in 8 characters.
nsrname=`echo "$username" |sed 's/^\(.\{1,8\}\).*$/\1/'`

# Now for the actual test
$at_traceoff
echo "basic.at:386: cvs log -N ssfile"
echo basic.at:386 >$at_check_line_file
( $at_traceon; cvs log -N ssfile ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:386: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


# Normalize the output of the previous command
sed -e "s,^date: 2[0-9][0-9][0-9]/[01][0-9]/[0-3][0-9] [0-2][0-9]:[0-6][0-9]:[0-6][0-9];  author: $nsrname;  ,date: DATE;  author: USERNAME;  ," \
    <stdout >nstdout
rm stdout

$at_traceoff
echo "basic.at:386: (cat nstdout; cat stderr >&2)"
echo basic.at:386 >$at_check_line_file
( $at_traceon; (cat nstdout; cat stderr >&2) ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "
RCS file: $CVSROOT_DIR/project/sdir/ssdir/ssfile,v
Working file: ssfile
head: 3.1
branch:
locks: strict
access list:
keyword substitution: kv
total revisions: 3;	selected revisions: 3
description:
----------------------------
revision 3.1
date: DATE;  author: USERNAME;  state: Exp;  lines: +0 -0
bump-it
----------------------------
revision 1.2
date: DATE;  author: USERNAME;  state: Exp;  lines: +1 -0
modify-it
----------------------------
revision 1.1
date: DATE;  author: USERNAME;  state: Exp;
add-it
=============================================================================
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:386: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

rm stderr nstdout

$at_traceoff
echo "basic.at:389: cvs -q update -p -r 1.1 ./ssfile"
echo basic.at:389 >$at_check_line_file
( $at_traceon; cvs -q update -p -r 1.1 ./ssfile ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "ssfile
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:389: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


      $at_traceoff
      $at_times_p && times >$at_times_file
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat $at_status_file`
    ;;

  13 ) # 13. basic.at:395: annotate
    at_setup_line='basic.at:395'
    at_desc='annotate'
    $at_quiet $ECHO_N " 13: annotate                                     $ECHO_C"
    at_xfail=no
    (
      echo "13. basic.at:395: testing ..."
      $at_traceon
###
### Begin AT_CVS_SETUP([annotate])
