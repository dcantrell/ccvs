# $SPROG add: use \`$SPROG commit' to add this file permanently
# ])
#
# to test: sub1/sub2/sub3

      $at_traceoff
      $at_times_p && times >$at_times_file
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat $at_status_file`
    ;;

  16 ) # 16. basic.at:671: delete last revision
    at_setup_line='basic.at:671'
    at_desc='delete last revision'
    $at_quiet $ECHO_N " 16: delete last revision                         $ECHO_C"
    at_xfail=no
    (
      echo "16. basic.at:671: testing ..."
      $at_traceon
###
### Begin AT_CVS_SETUP([delete last revision])
###


#
# Set CVSROOT_DIR, method, & CVSROOT
#
CVSROOT_DIR=$at_group_dir/cvsroot
method=
CVSROOT=$method$CVSROOT_DIR
export CVSROOT
SPROG=`basename $server`

#
# Clear a possibly damaging inherited environment.
#

# Avoid picking up any stray .cvsrc, etc., from the user running the tests
mkdir home
HOME=$at_group_dir/home; export HOME

# Make sure this variable is not defined to anything that would
# change the format of RCS dates.  Otherwise people using e.g.,
# RCSINIT=-zLT get lots of spurious failures.
RCSINIT=

#
# Initialize the repository
#
$at_traceoff
echo "basic.at:671: cvs init"
echo basic.at:671 >$at_check_line_file
( $at_traceon; cvs init ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:671: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


#
# Create the default project
#
mkdir top
cd top
$at_traceoff
echo "basic.at:671: cvs -Q co -l ."
echo basic.at:671 >$at_check_line_file
( $at_traceon; cvs -Q co -l . ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:671: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

mkdir project
$at_traceoff
echo "basic.at:671: cvs -Q add project"
echo basic.at:671 >$at_check_line_file
( $at_traceon; cvs -Q add project ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:671: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

cd ..
rm -r top

#
# Check out the default project and cd into the workspace.
#
# I'm checking for error returns form the cd and printing error messages since,
# for instance, if the CVS executable picks up a ~/.cvsrc which causes empty
# directories to be pruned by checkout, the project directory will not be
# created but the `cvs co' will not return an error.
#
$at_traceoff
echo "basic.at:671: cvs -Q co project"
echo basic.at:671 >$at_check_line_file
( $at_traceon; cvs -Q co project ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:671: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

cd project ||
  { { echo "$as_me:$LINENO: error: Couldn't cd to \\`project'.  Did \$HOME get set incorrectly?" >&5
echo "$as_me: error: Couldn't cd to \\`project'.  Did \$HOME get set incorrectly?" >&2;}
   { (exit 1); exit 1; }; }

###
### End AT_CVS_SETUP([delete last revision])
###



mkdir sdir
$at_traceoff
echo "basic.at:675: cvs add sdir"
echo basic.at:675 >$at_check_line_file
( $at_traceon; cvs add sdir ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "Directory $CVSROOT_DIR/project/sdir added to the repository
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:675: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

cd sdir
mkdir ssdir
$at_traceoff
echo "basic.at:675: cvs add ssdir"
echo basic.at:675 >$at_check_line_file
( $at_traceon; cvs add ssdir ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "Directory $CVSROOT_DIR/project/sdir/ssdir added to the repository
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:675: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

cd ssdir
echo ssfile >ssfile
$at_traceoff
echo "basic.at:675: cvs add ssfile"
echo basic.at:675 >$at_check_line_file
( $at_traceon; cvs add ssfile ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo stderr:; tee stderr <$at_stderr
echo stdout:; tee stdout <$at_stdout
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:675: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


sed \
    -e "s/\<[0-9][0-9]* [a-zA-Z][a-zA-Z]* [0-9][0-9]* [0-9:][0-9:]* -0000\>/RFCDATE/" \
    -e "s/^P /U /" \
    <stdout >nstdout
rm stdout

$at_traceoff
echo "basic.at:675: (cat nstdout; cat stderr >&2)"
echo basic.at:675 >$at_check_line_file
( $at_traceon; (cat nstdout; cat stderr >&2) ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo >>$at_stderr; echo "$SPROG add: scheduling file \`ssfile' for addition
$SPROG add: use \`$SPROG commit' to add this file permanently
" | $at_diff - $at_stderr || at_failed=:
$at_diff $at_devnull $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:675: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

rm stderr nstdout
$at_traceoff
echo "basic.at:675: cvs -q ci -m add-it"
echo basic.at:675 >$at_check_line_file
( $at_traceon; cvs -q ci -m add-it ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "RCS file: $CVSROOT_DIR/project/sdir/ssdir/ssfile,v
done
Checking in ssfile;
$CVSROOT_DIR/project/sdir/ssdir/ssfile,v  <--  ssfile
initial revision: 1.1
done
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:675: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon

cd ../..


cd sdir/ssdir

# Try to remove all revisions in a file.
$at_traceoff
echo "basic.at:685: cvs admin -o1.1 ssfile"
echo basic.at:685 >$at_check_line_file
( $at_traceon; cvs admin -o1.1 ssfile ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
echo >>$at_stderr; echo "$SPROG [admin aborted]: attempt to delete all revisions
" | $at_diff - $at_stderr || at_failed=:
echo >>$at_stdout; echo "RCS file: $CVSROOT_DIR/project/sdir/ssdir/ssfile,v
deleting revision 1.1
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   1) ;;
   *) echo "basic.at:685: exit code was $at_status, expected 1"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


echo I will be deleted second. >>ssfile
$at_traceoff
echo "basic.at:693: cvs -q ci -mr2"
echo basic.at:693 >$at_check_line_file
( $at_traceon; cvs -q ci -mr2 ) >$at_stdout 2>$at_stder1
at_status=$?
grep '^ *+' $at_stder1 >&2
grep -v '^ *+' $at_stder1 >$at_stderr
at_failed=false
$at_diff $at_devnull $at_stderr || at_failed=:
echo >>$at_stdout; echo "Checking in ssfile;
$CVSROOT_DIR/project/sdir/ssdir/ssfile,v  <--  ssfile
new revision: 1.2; previous revision: 1.1
done
" | $at_diff - $at_stdout || at_failed=:
case $at_status in
   77) echo 77 > $at_status_file
            exit 77;;
   0) ;;
   *) echo "basic.at:693: exit code was $at_status, expected 0"
      at_failed=:;;
esac
if $at_failed; then

  echo 1 > $at_status_file
  exit 1
fi

$at_traceon


echo I will be deleted first. >>ssfile
echo "It's kinda like seeing the future." >>ssfile
$at_traceoff
